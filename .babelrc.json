{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [
          "edge >= 16",
          "safari >= 9",
          "firefox >= 57",
          "ie >= 11",
          "ios >= 9",
          "chrome >= 49"
        ],
        "useBuiltIns": "usage",
        "corejs": {
          "version": "3",
          "proposals": true
        },
        "forceAllTransforms": true
      }
    ]
  ]
}
